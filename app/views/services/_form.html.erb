<div class="container">
  <div>
    <%= simple_form_for [@user, @service] do |f| %>
      <%= f.input :date %>
      <%= f.input :period, collection: ['9h - 10h', '10h - 11h','11h - 14h', '15h - 16h ', '16h - 17h'] %>
      <%= f.association :user_tasks, collection: @user.tasks, as: :check_boxes %>
      <p></p>
      <%= f.submit %>
    <% end %>
   </div>
</div>

